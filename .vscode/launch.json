{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "QEMU Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Qemu/qemu-system-i386w.exe", // QEMU 可执行文件路径
      "args": [
        "-S",
        "-gdb",
        "tcp::1234",
        "hd.img"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Set breakpoint at main",
          "text": "break main"
        }
      ],
    }
  ]
}